{
  "name": "result-system-app",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start-dev": "npm run build-lib && NODE_ENV=development node socket-io.server.js & ng serve",
    "start": "node server.js",
    "build": "npm run build-lib && ng build",
    "build-lib": "ng build simple-table-lib",
    "watch": "ng build --watch --configuration development",
    "test-watch": "ng test --watch",
    "test": "ng test",
    "test-ci": "ng test --no-watch --progress --browsers=ChromeHeadlessCI ",
    "test-lib": "ng test simple-table-lib",
    "test-lib-ci": "ng test simple-table-lib  --no-watch --progress --browsers=ChromeHeadlessCI ",
    "lint": "eslint . --ext .js,.ts",
    "lint-fix": "eslint . --ext .js,.ts --fix",
    "format": "prettier --write \"./**/*.{ts,js,json,md}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.3",
    "@angular/common": "~12.0.3",
    "@angular/compiler": "~12.0.3",
    "@angular/core": "~12.0.3",
    "@angular/forms": "~12.0.3",
    "@angular/platform-browser": "~12.0.3",
    "@angular/platform-browser-dynamic": "~12.0.3",
    "@angular/router": "~12.0.3",
    "@nrwl/workspace": "^12.4.0",
    "bootswatch": "^5.0.1",
    "express": "^4.17.1",
    "path": "^0.12.7",
    "rxjs": "~6.6.0",
    "socket.io": "~3.1.1",
    "socket.io-client": "~3.1.1",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.0",
    "@angular-eslint/builder": "12.1.0",
    "@angular-eslint/eslint-plugin": "12.1.0",
    "@angular-eslint/eslint-plugin-template": "12.1.0",
    "@angular-eslint/schematics": "12.1.0",
    "@angular-eslint/template-parser": "12.1.0",
    "@angular/cli": "~12.1.0",
    "@angular/compiler-cli": "~12.0.3",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "@types/prettier": "^1.19.0",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "eslint": "^7.26.0",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.0",
    "prettier": "2.0.5",
    "eslint-config-prettier": "7.1.0",
    "typescript": "~4.2.3"
  }
